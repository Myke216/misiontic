@page
@model ProyectoCiclo3.App.Frontend.Pages.EditRutasModel
@{
    var rutas = Model.Ruta;
}
 
<div class="text-center">
<form method="POST">
  <label for="id" class="form-label">Id:</label>
  <input type="number" class="form-control" id="id" name="id" placeholder="Id" asp-for="@rutas.id">
  <label for="origen" class="form-label">Origen:</label>
  <input type="text" class="form-control" id="origen" name="origen" placeholder="Origen" asp-for="@rutas.origen">
  <label for="destino" class="form-label">Destino:</label>
  <input type="text" class="form-control" id="destino" name="destino" placeholder="Destino" asp-for="@rutas.destino">
  <label for="tiempo_estimado " class="form-label">Tiempo estimado:</label>
  <input type="text" class="form-control" id="tiempo_estimado" name="tiempo_estimado" placeholder="Tiempo estimado"asp-for="@rutas.tiempo_estimado">
  <hr>
  <button type="submit" class="btn btn-success">Enviar</button>
</form>
</div>
